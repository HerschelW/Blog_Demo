<%- include("partials/header.ejs"); %>
<div class="content">
   <h1>Home</h1>
   <p><%=homeContent%></p>
   <% posts.forEach(post => { %>
   <div class="blogContent">
      <h1><%= post.title %></h1>
      <p>
         <%=post.content.substring(0,100) + "..."%>
         <a href="/posts/<%=post.title%>">Read More</a>
      </p>
   </div>
   <%}); %>
</div>
<%- include("partials/footer.ejs"); %>
